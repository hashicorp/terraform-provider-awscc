        "ec2:DescribeVpnConcentrators"
      ]
    },
    "update": {
      "permissions": [
        "ec2:DescribeVpnConcentrators",
        "ec2:CreateTags",
        "ec2:DeleteTags"
      ]
    },
    "read": {
      "permissions": [
        "ec2:DescribeVpnConcentrators"
      ]
    },
    "list": {
      "permissions": [
        "ec2:DescribeVpnConcentrators"
      ]
    }
  }
