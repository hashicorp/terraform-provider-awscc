        },
        "Value": {
          "$ref": "#/definitions/MapValue"
        }
      },
      "additionalProperties": false
    },
    "MapFilterList": {
      "type": "array",
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/MapFilter"
      },
      "maxItems": 10,
      "minItems": 1
