    "Timestamp": {
      "type": "integer",
      "format": "int64"
    },
    "DateFilter": {
      "type": "object",
      "properties": {
        "EndInclusive": {
          "$ref": "#/definitions/Timestamp"
        },
        "StartInclusive": {
          "$ref": "#/definitions/Timestamp"
        }
      },
      "additionalProperties": false
    },
    "DateFilterList": {
      "type": "array",
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/DateFilter"
      },
      "maxItems": 10,
      "minItems": 1
