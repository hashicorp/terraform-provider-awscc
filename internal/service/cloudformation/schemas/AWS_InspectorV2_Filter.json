    "MapComparison": {
      "type": "string",
      "enum": [
        "EQUALS"
      ]
    },
    "MapFilter": {
      "type": "object",
      "required": [
        "Comparison"
      ],
      "properties": {
        "Comparison": {
          "$ref": "#/definitions/MapComparison"
