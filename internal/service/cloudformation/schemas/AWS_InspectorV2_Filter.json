          "type": "number"
        },
        "UpperInclusive": {
          "type": "number"
        }
      },
      "additionalProperties": false
    },
    "NumberFilterList": {
      "type": "array",
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/NumberFilter"
      },
      "maxItems": 10,
      "minItems": 1
