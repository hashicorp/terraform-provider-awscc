      "type": "array",
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/PortRangeFilter"
      },
      "maxItems": 10,
      "minItems": 1
    },
    "NumberFilter": {
      "type": "object",
      "properties": {
