      "permissions": [
        "inspector2:CreateFilter",
        "inspector2:ListFilters",
        "inspector2:TagResource"
      ]
    },
    "read": {
      "permissions": [
        "inspector2:ListFilters",
        "inspector2:ListTagsForResource"
      ]
    },
    "update": {
      "permissions": [
        "inspector2:ListFilters",
        "inspector2:UpdateFilter",
        "inspector2:TagResource",
        "inspector2:UntagResource",
        "inspector2:ListTagsForResource"
      ]
    },
    "delete": {
      "permissions": [
        "inspector2:DeleteFilter",
        "inspector2:ListFilters"
      ]
    },
    "list": {
