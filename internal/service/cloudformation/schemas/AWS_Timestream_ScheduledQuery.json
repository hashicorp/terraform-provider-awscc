        "ObjectKeyPrefix": {
            "description": "Prefix for error report names.",
            "type": "string",
            "minLength": 1,
            "maxLength": 896,
            "pattern": ""
        },
        "EncryptionOption": {
            "description": "How error reports will be encrypted.",
            "type": "string",
            "enum": ["SSE_S3", "SSE_KMS"]
        },
