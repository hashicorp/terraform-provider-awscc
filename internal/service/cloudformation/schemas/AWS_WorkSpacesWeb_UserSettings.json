        "Name": {
          "type": "string",
          "maxLength": 4096,
          "minLength": 0
        },
        "Path": {
          "type": "string",
          "maxLength": 2000,
          "minLength": 0,
          "pattern": "^/(\\S)*$"
        }
      },
      "required": [
        "Domain"
      ],
      "additionalProperties": false
    },
    "CookieSynchronizationConfiguration": {
      "type": "object",
