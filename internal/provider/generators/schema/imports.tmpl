[
{{- range $i, $e := .Resources }}
    {{- if $i}},{{- end}}
    {
        "resource": "{{ $e.ResourceName }}",{{if $e.GenerateListResource}}
        "generateListResource": {{ $e.GenerateListResource }},{{end}}
        "identifier": [{{- range $i, $v := $e.Identifier }} {{- if $i}},{{- end}}"{{ $v }}"{{- end}}]
    }
{{- end}}
]
